<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>HelloSevlet</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>DirectServletObj</servlet-name>
  <servlet-class>com.example.directservlet.DirectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>DirectServletBunnyBananas</servlet-name>
  <url-pattern>/dirserv</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>IndirectServletObj</servlet-name>
  	<servlet-class>com.example.indirectservlet.IndirectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>IndirectServletObj</servlet-name>
  	<servlet-class>IndriectServ</servlet-class>
  </servlet-mapping>
  
</web-app>